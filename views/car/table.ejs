<div class="container-fluid">
  <div class="d-flex justify-content-end">
    <a
      class="btn btn-primary align-self-end"
      role="button"
      href="/admin/dashboard/car/create"
      >Create Car</a
    >
  </div>
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Car Table</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table
          class="table table-bordered"
          id="dataTable"
          width="100%"
          cellspacing="0"
        >
          <thead>
            <tr>
              <th>Id</th>
              <th>Name</th>
              <th>Model</th>
              <th>Human</th>
            </tr>
          </thead>
          <tbody>
            <% cars.forEach(car => { %>
            <tr>
              <td><%= car.id %></td>
              <td><%= car.name %></td>
              <td><%= car.model %></td>
              <td><%= car.Human.name %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
