<div class="container-fluid">
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Token Table</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table
          class="table table-bordered"
          id="dataTable"
          width="100%"
          cellspacing="0"
        >
          <thead>
            <tr>
              <th>Id</th>
              <th>Created At</th>
              <th>Refresh Token</th>
            </tr>
          </thead>
          <tbody>
            <% tokens.forEach(token => { %>
            <tr>
              <td><%= token.id %></td>
              <td><%= token.createdAt %></td>
              <td><%= token.refreshToken %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
